{
  "name": "airtable-formula-builder",
  "description": "Helper tool to create formulas compatible with Airtable API `formulaField`.",
  "author": {
    "name": "Gabriel Almeida",
    "email": "gabriel_almeida@me.com"
  },
  "keywords": [
    "airtable",
    "formula",
    "builder",
    "query"
  ],
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
