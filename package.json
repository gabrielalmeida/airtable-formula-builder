    "testRegex": ".*\\.spec\\.ts$"
  },
  "dependencies": {
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/ramda": "^0.25.34",
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  }
}
